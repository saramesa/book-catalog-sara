export const getBooksCatalog = fetch("https://cors-anywhere.herokuapp.com/https://saramesa-book-catalog.firebaseapp.com/api/catalog")
      .then(res => res.json())
      .then(
        (result) => {
          this.setState({
            isLoaded: true,
            items: result.catalog
          });
        },
        (error) => {
          this.setState({
            isLoaded: true,
            error
          });
        }
      )